#include<stdio.h>
int main()
{
int m;
printf("Enter the Marks: ");
scanf("%d",&m);
if(m>=85 && m<=100)
printf("Grade A\n");
else if(m>=70 && m<=84)
printf("Grade B\n");
else if(m>=55 && m<=69)
printf("Grade C\n");
else if(m>=40 && m<=54)
printf("Grade D\n");
else if(m<40)
printf("Grade F\n");
return 0;
}
